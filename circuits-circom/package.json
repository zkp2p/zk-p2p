    "compile:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./1_compile.sh && cd ..",
    "compile:venmo:send:unsafe": "CIRCUIT_NAME=venmo_send && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "compile:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./1_compile.sh && cd ..",
    "compile:venmo:registration:unsafe": "CIRCUIT_NAME=venmo_registration && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "compile:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./1_compile.sh && cd ..",
    "compile:hdfc:send:unsafe": "CIRCUIT_NAME=hdfc_send && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "compile:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./1_compile.sh && cd ..",
    "compile:hdfc:registration:unsafe": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./1_compile_unsafe.sh && cd ..",
