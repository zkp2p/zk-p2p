    "richardliang",
    "0xSachinK"
  ],
  "main": "dist/generate_input.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@zk-email/circuits": "^2.0.1",
    "@zk-email/helpers": "^2.0.0",
    "buffer": "^6.0.3",
