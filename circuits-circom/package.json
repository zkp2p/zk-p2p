    "genkey:chunked:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "uploadkeys:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "uploadkeys:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "uploadkeys:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "uploadkeys:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./7_upload_to_s3.sh && cd ..",
