    "test:venmo:registration": "yarn test ./test/venmo_registration.spec.ts",
    "test:hdfc:send": "yarn test ./test/hdfc_send.spec.ts",
    "test:hdfc:registration": "yarn test ./test/hdfc_registration.spec.ts",
    "genverifier:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "genverifier:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "genverifier:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "genverifier:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "gencalldata:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "gencalldata:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "gencalldata:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "gencalldata:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "genwitness:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genwitness:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genwitness:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genwitness:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genproof:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
    "genproof:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
    "genproof:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
    "genproof:hdfc:registration": "CIRCUIT_NAME=hdfc_registration && cd scripts && ./6_gen_proof_rapidsnark.sh && cd .."
