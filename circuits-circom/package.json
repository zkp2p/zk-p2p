    "genkey:chunked:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:venmo:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "genkey:both:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_both_zkeys.sh && cd ..",
    "genkey:chunked:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:venmo:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "genkey:chunked:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:hdfc:send": "CIRCUIT_NAME=hdfc_send && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
