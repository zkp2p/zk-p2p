{
  "name": "@zkp2p/circuits-circom",
  "version": "0.0.6",
  "packageManager": "yarn@3.6.3",
  "description": "Circom circuits for ZKP2P",
  "contributors": [
    "richardliang",
    "0xSachinK"
  ],
  "main": "dist/generate_input.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "dependencies": {
    "@zk-email/circuits": "^2.0.0",
    "@zk-email/helpers": "^1.0.3",
    "buffer": "^6.0.3",
    "circomlib": "^2.0.5",
    "ethers": "^5.5.1",
    "process": "^0.11.10",
    "snarkjs": "git+https://github.com/vb7401/snarkjs.git#24981febe8826b6ab76ae4d76cf7f9142919d2b8"
  },
  "scripts": {
    "gen-input": "npx tsx ./scripts/generate_input.ts",
    "gen-input:receive": "npx tsx ./scripts/generate_input.ts venmo_receive EMAIL_VENMO_RECEIVE",
    "gen-input:send": "npx tsx ./scripts/generate_input.ts venmo_send EMAIL_VENMO_SEND",
    "gen-input:reg-complete": "npx tsx ./scripts/generate_input.ts venmo_complete EMAIL_VENMO_REGISTRATION",
    "gen-input:reg-send": "npx tsx ./scripts/generate_input.ts venmo_send EMAIL_VENMO_REGISTRATION",
    "gen-input:reg-receive": "npx tsx ./scripts/generate_input.ts venmo_receive EMAIL_VENMO_REGISTRATION",
    "test": "jest",
    "build": "tsc",
    "compile:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./1_compile.sh && cd ..",
    "compile:receive:unsafe": "CIRCUIT_NAME=venmo_receive && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "compile:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./1_compile.sh && cd ..",
    "compile:send:unsafe": "CIRCUIT_NAME=venmo_send && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "compile:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./1_compile.sh && cd ..",
    "compile:registration:unsafe": "CIRCUIT_NAME=venmo_registration && cd scripts && ./1_compile_unsafe.sh && cd ..",
    "genkey:both:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./3_gen_both_zkeys.sh && cd ..",
    "genkey:chunked:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "genkey:both:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_both_zkeys.sh && cd ..",
    "genkey:chunked:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "genkey:both:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_both_zkeys.sh && cd ..",
    "genkey:chunked:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_chunk_zkey.sh && cd ..",
    "genkey:chunked:unsafe:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_chunk_zkey_unsafe.sh && cd ..",
    "genkey:non-chunked:unsafe:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./3_gen_zkey_unsafe.sh && cd ..",
    "uploadkeys:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "uploadkeys:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "uploadkeys:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./7_upload_to_s3.sh && cd ..",
    "test:receive": "yarn test ./test/venmo_receive.spec.ts",
    "test:send": "yarn test ./test/venmo_send.spec.ts",
    "test:registration": "yarn test ./test/venmo_registration.spec.ts",
    "genverifier:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "genverifier:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "genverifier:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./8_gen_sol_verifier.sh && cd ..",
    "gencalldata:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "gencalldata:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "gencalldata:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./9_gen_sol_calldata.sh && cd ..",
    "genwitness:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genwitness:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genwitness:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./2_gen_wtns.sh && cd ..",
    "genproof:send": "CIRCUIT_NAME=venmo_send && cd scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
    "genproof:receive": "CIRCUIT_NAME=venmo_receive && cd scripts && ./6_gen_proof_rapidsnark.sh && cd ..",
    "genproof:registration": "CIRCUIT_NAME=venmo_registration && cd scripts && ./6_gen_proof_rapidsnark.sh && cd .."
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.21.5",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@openzeppelin/contracts": "^4.9.1",
    "@types/atob": "^2.1.2",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.6",
    "@types/node-forge": "^1.3.2",
    "@types/tar-stream": "^2.2.2",
    "babel-preset-jest": "^29.5.0",
    "browserify-fs": "^1.0.0",
    "browserstack-local": "^1.5.1",
    "browserstack-node-sdk": "^1.6.1",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.19",
    "ganache": "^7.3.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^15.0.0",
    "madge": "^6.0.0",
    "mocha": "^10.2.0",
    "msw": "^1.0.1",
    "nodemon": "^2.0.19",
    "process": "^0.11.10",
    "puppeteer": "18.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "selenium-webdriver": "^4.8.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6",
    "util": "^0.12.5",
    "vite-plugin-commonjs": "^0.7.1",
    "vite-plugin-ngmi-polyfill": "^0.0.2",
    "vite-plugin-node-polyfills": "^0.8.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transformIgnorePatterns": [
      "node_modules/(?!(@zk-email)/)"
    ]
  }
}
